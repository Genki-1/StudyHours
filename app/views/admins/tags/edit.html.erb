<div class="container">
  <h3 class="row">チャートグラフのカラーコード変更</h3>
  <div class="row mt-4">
    <div class="col-8">
      <%= form_with model: @tag, url: admins_tag_path(@tag) do |f|%>
        <%= f.label :color_code, "カラーコード" %>
        <%= f.text_field :color_code %>
        <%= f.submit "登録",class: "btn btn-primary" %>
      <% end %>      
    </div>
  </div>
</div>